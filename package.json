{
  "name": "demo-cordinates-api",
  "version": "1.0.0",
  "description": "An API app that allows geofencing and geofiltering",
  "main": "index.js",
  "scripts": {
    "dev": "NODE_ENV=dev nodemon --exec babel-node -- src/index.js",
    "build": "babel src -s -D -d dist --presets es2015,stage-0",
    "start": "NODE_ENV=production node dist",
    "prestart": "npm run -s build",
    "lint": "eslint src",
    "pretest": "mongoimport --db test --collection geo --drop --file src/data.json",
    "test": "NODE_ENV=test mocha src --recursive --timeout 10000 --compilers js:babel-register"
  },
  "dependencies": {
    "body-parser": "^1.17.2",
    "chai": "^4.0.0",
    "chai-http": "^3.0.0",
    "chai-json-schema": "^1.4.0",
    "compression": "^1.5.2",
    "cookie-parser": "^1.4.3",
    "cors": "^2.7.1",
    "debug": "^2.6.3",
    "dotenv": "^4.0.0",
    "express": "^4.15.3",
    "geolib": "^2.0.22",
    "geopoint": "^1.0.1",
    "isobject": "^3.0.0",
    "map-async": "^0.1.1",
    "mocha": "^3.4.2",
    "mongodb": "^2.2.28",
    "mongoose": "^4.10.4",
    "morgan": "^1.8.1",
    "resource-router-middleware": "^0.6.0"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb-base": "^11.2.0",
    "eslint-plugin-import": "^2.3.0",
    "nodemon": "^1.11.0"
  },
  "peerDependencies": {
    "chai": "3.5.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/musale/demo-cordinates-api.git"
  },
  "keywords": [
    "nodejs",
    "mongodb",
    "express"
  ],
  "author": "Musale Martin",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/musale/demo-cordinates-api/issues"
  },
  "homepage": "https://github.com/musale/demo-cordinates-api#readme"
}
